<einit prefix="services-virtual-module">
 <shell id="shell-sysctl"
  name="configure kernel parameters at runtime (sysctl)"
  provides="sysctl"
  requires="mount-critical"
  enable="sysctl -q -p ${configuration_services_sysctl_config}"
  disable="/bin/true"
  restart="no"
  variables="configuration-services-sysctl/.*" />
</einit>
