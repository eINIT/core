<einit prefix="services-virtual-module">
 <shell id="shell-audio-alsa"
  name="Audio (ALSA)"
  provides="alsa"
  requires="mount-critical"
  after="kern-alsa"
  enable="/usr/sbin/alsactl -f ${configuration_services_alsa_mixer_file} restore;"
  disable="/usr/sbin/alsactl -f ${configuration_services_alsa_mixer_file} store;"
  variables="configuration-services-alsa-mixer/.*" />
</einit>
