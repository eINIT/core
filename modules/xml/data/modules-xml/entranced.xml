<module>
 <shell id="shell-entranced"
  name="EntranceD (TEST SHELL VERSION)"
  provides="x11:displaymanager"
  requires="mount-critical:fqdn"
  after="^net-lo$"
  enable="/usr/sbin/entranced"
  disable="killall -9 entranced"
  DISPLAY=":0.0"
  HOME="/root"
  need-files="/usr/sbin/entranced" />
</module>
