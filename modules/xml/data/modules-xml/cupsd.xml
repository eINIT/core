<module>
 <v-cupsd
  name="Common Unix Printing System Daemon (cupsd)"
  provides="cupsd"
  requires="mount-critical">

  <execute id="prepare" code="if [ ! -d /var/log/cups ]; then mkdir /var/log/cups; fi; /bin/true" />
  <execute id="daemon" code="cupsd -F" />

  <restart b="yes" />
  <need-files s="cupsd" />
 </v-cupsd>
</module>
