<module>
 <daemon id="daemon-cupsd"
  name="Common Unix Printing System Daemon (cupsd)"
  provides="cupsd"
  requires="network-minimal:mount-critical"
  prepare="if [ ! -d /var/log/cups ]; then mkdir /var/log/cups; fi; /bin/true"
  command="/usr/sbin/cupsd -F"
  restart="yes"
  need-files="/usr/sbin/cupsd" />
</module>
